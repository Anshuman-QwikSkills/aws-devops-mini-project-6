pipeline{
    agent any

	stages {
	    
	    stage('gitclone') {

			steps {
				git 'https://github.com/Anshuman-QwikSkills/aws-devops-mini-project-6.git'
			}
		}

		stage('Build') {

			steps {
				sh 'docker build -t bharathsharath/frontend ./frontend'
				sh 'docker build -t bharathsharath/backend ./backend'
			}
		}
        
    }
}
